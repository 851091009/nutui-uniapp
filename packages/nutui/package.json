{
  "name": "uniapp-nutui",
  "type": "module",
  "version": "0.0.1",
  "description": "京东风格的轻量级移动端 Uniapp、Vue3 组件库（支持小程序开发）",
  "author": "yang1206 <y1149221897@outlook.com>",
  "license": "MIT",
  "keywords": [
    "uniapp",
    "nutui",
    "nutui4",
    "vue",
    "vue3",
    "vue component",
    "vue3 component",
    "vite"
  ],
  "sideEffects": [
    "*.scss",
    "*.css",
    "./_locale/**"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    },
    "./package.json": "./package.json",
    "./locale": {
      "types": "./locale/index.d.ts",
      "require": "./locale/index.cjs",
      "import": "./locale/index.mjs"
    },
    "./*": "./*"
  },
  "main": "src/index.ts",
  "publishConfig": {
    "main": "./dist/index.cjs",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts"
  },
  "files": [
    "dist",
    "style",
    "components",
    "locale/**"
  ],
  "scripts": {
    "build": "unbuild",
    "stub": "unbuild --stub",
    "build:css": "vite build --config vite.config.build.css.ts"
  },
  "peerDependencies": {
    "vue": "^3.2.0"
  }
}
