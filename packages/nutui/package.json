{
  "name": "uniapp-nutui",
  "type": "module",
  "version": "0.0.4",
  "description": "京东风格的轻量级移动端 Uniapp、Vue3 组件库（支持小程序开发）",
  "author": "yang1206 <y1149221897@outlook.com>",
  "license": "MIT",
  "homepage": "https://github.com/yang1206/uniapp-nutui#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yang1206/uniapp-nutui.git"
  },
  "bugs": "https://github.com/yang1206/uniapp-nutui/issues",
  "keywords": [
    "uniapp",
    "nutui",
    "nutui4",
    "vue",
    "vue3",
    "vue component",
    "vue3 component",
    "vite"
  ],
  "sideEffects": [
    "*.scss",
    "./_locale/**",
    "*/components/**/*.vue"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    },
    "./package.json": "./package.json",
    "./locale": {
      "types": "./locale/index.d.ts",
      "import": "./locale/index.mjs"
    },
    "./*": "./*"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "publishConfig": {
    "main": "./dist/index.cjs",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts"
  },
  "files": [
    "dist",
    "styles",
    "components",
    "README.md",
    "locale/**"
  ],
  "scripts": {
    "build": "unbuild",
    "stub": "unbuild --stub"
  },
  "peerDependencies": {
    "vue": "^3.2.0"
  }
}
