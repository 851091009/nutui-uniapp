{
  "extends": "@vue/tsconfig/tsconfig.json",
  "compilerOptions": {
    "sourceMap": true,
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/*"
      ]
    },
    "lib": [
      "esnext",
      "dom"
    ],
    "types": [
      "nutui-uniapp/global.d.ts",
      "@dcloudio/types",
      "@uni-helper/uni-app-types"
    ]
  },
  "include": [
    // "node_modules/nutui-uniapp/global.d.ts",
    "src/**/*.vue",
    "src/**/*.ts",
    "src/**/*.js",
    "src/**/*.d.ts",
    "src/**/*.tsx",
    "src/utils/touchEmulator.js"
  ]
}
