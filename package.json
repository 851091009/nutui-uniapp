{
  "name": "nutui",
  "version": "0.0.6",
  "description": "",
  "author": "",
  "license": "MIT",
  "keywords": [
    "nutui",
    "vue3",
    "uniapp",
    "typescript"
  ],
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "pnpm run --dir packages/nutui build",
    "dev:mp-weixin": "npm -C example run dev:mp-weixin",
    "build:mp-weixin": "pnpm build && npm -C example run build:mp-weixin",
    "dev:h5": "npm -C example run dev:h5",
    "build:h5": "npm -C example run build:h5",
    "dev:app": "npm -C example run dev:app",
    "build:app": "npm -C example run build:app",
    "docs:dev": "npm -C docs run dev",
    "docs:build": "npm -C docs run build",
    "docs:preview": "npm -C docs run serve",
    "lint": "eslint . --fix",
    "release": "bumpp package.json packages/*/package.json example/package.json",
    "lint:style": "stylelint --cache \"**/*.{vue,scss}\" --fix",
    "postinstall": "npx simple-git-hooks",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md",
    "commit": "git pull && git add -A && git-cz && git push",
    "build:all": "pnpm build && pnpm build:h5 && pnpm docs:build && esno scripts/uni.ts"
  },
  "dependencies": {
    "vue": "3.2.47"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.39.5",
    "@commitlint/cli": "^17.6.5",
    "@commitlint/config-conventional": "^17.6.5",
    "@dcloudio/types": "^3.3.3",
    "@nutui/touch-emulator": "^1.0.0",
    "@types/node": "^18.15.0",
    "@uni-helper/uni-app-types": "^0.5.8",
    "@vue/tsconfig": "^0.4.0",
    "bumpp": "^9.1.1",
    "conventional-changelog-cli": "^3.0.0",
    "cz-git": "^1.6.1",
    "eslint": "^8.42.0",
    "esno": "^0.16.3",
    "lint-staged": "^13.2.2",
    "postcss": "^8.4.24",
    "postcss-html": "^1.5.0",
    "sass": "^1.62.1",
    "simple-git-hooks": "^2.8.1",
    "stylelint": "^15.7.0",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-recess-order": "^4.2.0",
    "stylelint-config-recommended": "^12.0.0",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard": "^33.0.0",
    "stylelint-config-standard-scss": "^9.0.0",
    "stylelint-order": "^6.0.3",
    "typescript": "5.0.4",
    "unbuild": "^1.2.1",
    "uni-tsc": "^0.0.2",
    "vite": "4.3.9",
    "vitepress": "1.0.0-beta.5",
    "vitest": "^0.32.2",
    "vue-tsc": "1.6.5"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*.{vue,js,ts}": "eslint --fix",
    "*.{scss,css}": "stylelint --fix"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git",
      "useEmoji": true
    }
  }
}
